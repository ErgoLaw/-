		<field xsi:type="DataSetFieldField">
			<dataPath>Партия</dataPath>
			<field>Партия</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ОстаткиОбороты.Склад КАК Склад,
	ОстаткиОбороты.Товар КАК Товар,
	ЕСТЬNULL(ОстаткиОбороты.КоличествоПриход, 0) КАК КоличествоПриход,
	ЕСТЬNULL(ОстаткиОбороты.КоличествоРасход, 0) КАК КоличествоРасход,
	ЕСТЬNULL(ОстаткиОстатки.КоличествоОстаток, 0) КАК КоличествоОстаток
ИЗ
	РегистрНакопления.Остатки.Обороты(
			,
			,
			,
			Склад В
				(ВЫБРАТЬ
					Склады.Ссылка КАК Ссылка
				ИЗ
					Справочник.Склады КАК Склады)) КАК ОстаткиОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.Остатки.Остатки(
				&amp;ДатаОстаткиКонец,
				Склад В
					(ВЫБРАТЬ
						Склады.Ссылка КАК Ссылка
					ИЗ
						Справочник.Склады КАК Склады)) КАК ОстаткиОстатки
		ПО ОстаткиОбороты.Склад = ОстаткиОстатки.Склад
			И ОстаткиОбороты.Товар = ОстаткиОстатки.Товар

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ОстаткиОбороты.Салон,
	ОстаткиОбороты.Товар,
	ЕСТЬNULL(ОстаткиОбороты.КоличествоПриход, 0),
	ЕСТЬNULL(ОстаткиОбороты.КоличествоРасход, 0),
	ЕСТЬNULL(ОстаткиОстатки.КоличествоОстаток, 0)
ИЗ
	РегистрНакопления.Остатки.Обороты(
			,
			,
			,
			Салон В
				(ВЫБРАТЬ
					Салоны.Ссылка КАК Ссылка
				ИЗ
					Справочник.Салоны КАК Салоны)) КАК ОстаткиОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.Остатки.Остатки(
				&amp;ДатаОстаткиКонец,
				Салон В
					(ВЫБРАТЬ
						Салоны.Ссылка КАК Ссылка
					ИЗ
						Справочник.Салоны КАК Салоны)) КАК ОстаткиОстатки
		ПО ОстаткиОбороты.Салон = ОстаткиОстатки.Салон
			И ОстаткиОбороты.Товар = ОстаткиОстатки.Товар</query>
